<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".MapsActivity.InventoryActivity">

    <Button
        android:id="@+id/button"
        android:layout_width="418dp"
        android:layout_height="52dp"
        android:layout_marginBottom="8dp"
        android:text="@string/generaterandtoken"
        app:layout_constraintBottom_toTopOf="@+id/recyclerViewInventory"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recyclerViewInventory"
        android:layout_width="363dp"
        android:layout_height="559dp"
        android:layout_margin="8dp"
        android:layout_marginBottom="32dp"
        android:clipToPadding="false"
        android:padding="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button"
        tools:listitem="@layout/item_inventory_token" />

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/bottom_navigation_fragment"
        android:name="edu.uark.ahnelson.openstreetmap2024.MapsActivity.BottomNavigationFragment"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/recyclerViewInventory" />

</androidx.constraintlayout.widget.ConstraintLayout>